{
  "name": "mmo-packman",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "npm run dev:client",
    "dev:client": "vite",
    "build": "npm run build:client",
    "build:client": "vite build",
    "check": "npm run generate:game-rules:check && tsc -p tsconfig.client.json --noEmit",
    "test": "npm run generate:game-rules:check && npm run test:generator && npm run test:parity:selftest && npm run test:ping-manager && npm run test:replay-parser",
    "test:generator": "node scripts/generate-game-rules.selftest.mjs",
    "test:parity": "tsx reference/ts-server/src/server/parity_harness.ts",
    "test:parity:selftest": "tsx reference/ts-server/src/server/parity_harness.selftest.ts",
    "test:ping-manager": "tsx reference/ts-server/src/server/ping_manager.selftest.ts",
    "test:replay-parser": "tsx src/client/replay_parser.selftest.ts",
    "generate:game-rules": "node scripts/generate-game-rules.mjs",
    "generate:game-rules:check": "node scripts/generate-game-rules.mjs --check",
    "simulate": "npm run simulate:rust --",
    "simulate:rust": "cargo run --manifest-path rust/server/Cargo.toml --bin simulate --",
    "reference:ts:simulate": "tsx reference/ts-server/src/server/simulate.ts",
    "start": "echo \"Rust WebSocket server is not implemented yet. Use simulator commands for now.\" && exit 1"
  },
  "dependencies": {
    "express": "^5.1.0",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@types/express": "^5.0.4",
    "@types/node": "^24.3.1",
    "@types/ws": "^8.18.1",
    "concurrently": "^9.2.1",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2",
    "vite": "^7.3.1"
  }
}
